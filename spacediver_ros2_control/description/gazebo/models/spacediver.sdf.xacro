<?xml version="1.0" ?>
<sdf version="1.5" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find spacediver_ros2_control)/description/urdf/macros/properties.xacro"/>
  <model name="my1stmodel">
    <!-- ros_control plugin -->
    <plugin filename="libgazebo_ros2_control.so" name="gazebo_ros2_control">
      <parameters>$(find spacediver_ros2_control)/bringup/config/spacediver_controllers.yaml</parameters>
    </plugin>

    <!-- Model -->
    <!-- parallel link is defined here but not in urdf -->
    <!-- active joints hsould have the same name as those in urdf -->
    <static>false</static>
    <link name="base_link">
      <!-- <pose>0 0 0 0 0 0</pose> -->
      <collision name="collision">
        <geometry>
          <box>
            <size>${BASE_X} ${BASE_Y} ${BASE_Z}</size>
          </box>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100</mu>
              <mu2>50</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
            <size>${BASE_X} ${BASE_Y} ${BASE_Z}</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material>
      </visual>
    </link>

    <link name="left_shoulder_plate">
      <pose>
        ${SHOULDER_PLATE_POS_X} ${SHOULDER_PLATE_POS_Y} ${SHOULDER_PLATE_POS_Z}
        ${SHOULDER_PLATE_ROT_X} ${SHOULDER_PLATE_ROT_Y} ${SHOULDER_PLATE_ROT_Z}
      </pose>
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://spacediver/meshes/shoulder_plate.STL</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://spacediver/meshes/shoulder_plate.STL</uri>
          </mesh>
        </geometry>
        <!-- <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Grey</name>
          </script>
        </material> -->
      </visual>
    </link>

    <link name="left_dynamixel_top">
      <pose>
        ${DYNAMIXEL_LEFT_TOP_POS_X} ${DYNAMIXEL_LEFT_TOP_POS_Y} ${DYNAMIXEL_LEFT_TOP_POS_Z}
      </pose>
      <collision name="collision">
        <geometry>
          <box>
            <size>${DYNAMIXEL_X} ${DYNAMIXEL_Y} ${DYNAMIXEL_Z}</size>
          </box>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100</mu>
              <mu2>50</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
            <size>${DYNAMIXEL_X} ${DYNAMIXEL_Y} ${DYNAMIXEL_Z}</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Blue</name>
          </script>
        </material>
      </visual>
    </link>

    <link name="left_dynamixel_bottom">
      <pose>
        ${DYNAMIXEL_LEFT_BOT_POS_X} ${DYNAMIXEL_LEFT_BOT_POS_Y} ${DYNAMIXEL_LEFT_BOT_POS_Z}
      </pose>
      <collision name="collision">
        <geometry>
          <box>
            <size>${DYNAMIXEL_X} ${DYNAMIXEL_Y} ${DYNAMIXEL_Z}</size>
          </box>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100</mu>
              <mu2>50</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
            <size>${DYNAMIXEL_X} ${DYNAMIXEL_Y} ${DYNAMIXEL_Z}</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Blue</name>
          </script>
        </material>
      </visual>
    </link>

    <link name="misumi_ln">
      <pose>
        ${MISUMILN_POS_X} ${MISUMILN_POS_Y} ${MISUMILN_POS_Z}
        0 0 ${MISUMILN_ROT_Z}
      </pose>
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://spacediver/meshes/Link_MISUMI_LNMS16-10-200.STL</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://spacediver/meshes/Link_MISUMI_LNMS16-10-200.STL</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Red</name>
          </script>
        </material>
      </visual>
    </link>

    <link name="driving_link">
      <pose>
        ${DRIVING_POS_X} ${DRIVING_POS_Y} ${DRIVING_POS_Z}
      </pose>
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://spacediver/meshes/upper_arm_driving_link.STL</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://spacediver/meshes/upper_arm_driving_link.STL</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Green</name>
          </script>
        </material>
      </visual>
    </link>

    <link name="driven_link">
      <pose>
        ${DRIVEN_POS_X} ${DRIVEN_POS_Y} ${DRIVEN_POS_Z}
        0 0 ${DRIVEN_ROT_Z}
      </pose>
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://spacediver/meshes/upper_arm_driven_link.STL</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://spacediver/meshes/upper_arm_driven_link.STL</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Red</name>
          </script>
        </material>
      </visual>
    </link>

    <link name="misumi_ft">
      <pose>
        ${MISUMIFT_POS_X} ${MISUMIFT_POS_Y} ${MISUMIFT_POS_Z}
        0 0 ${MISUMIFT_ROT_Z}
      </pose>
      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://spacediver/meshes/Misumi_LNFTS10-P10-V10-L200-S50-W18-T9.STL</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://spacediver/meshes/Misumi_LNFTS10-P10-V10-L200-S50-W18-T9.STL</uri>
          </mesh>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Green</name>
          </script>
        </material>
      </visual>
    </link>


    <joint name="dynamixel_to_driving_joint" type="revolute">
      <parent>left_dynamixel_top</parent>
      <child>driving_link</child>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>

    <joint name="driving_to_driven_joint" type="revolute">
      <parent>driving_link</parent>
      <child>driven_link</child>
      <axis>
        <xyz>0 0 1</xyz>
      </axis>
    </joint>
  </model>
</sdf>